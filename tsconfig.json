{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "lib": ["es2015", "es2016", "esnext"],
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "declaration": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node"
  }
}
